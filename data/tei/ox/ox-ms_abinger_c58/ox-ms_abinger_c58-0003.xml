<surface xmlns="http://www.tei-c.org/ns/1.0" xmlns:sga="http://shelleygodwinarchive.org/ns/1.0"
  xml:id="ox-ms_abinger_c58-0003" ulx="0" uly="0" lrx="5410" lry="6660" partOf="#ox-ms_abinger_c58">
