<surface xmlns="http://www.tei-c.org/ns/1.0" xmlns:sga="http://sga.mith.org/ns/1.0" ulx="0" uly="0"
  lrx="5406" lry="7276" xml:id="ox-ms_shelley_e1-0007" partOf="#ox-ms_shelley_e1">
