<surface xmlns="http://www.tei-c.org/ns/1.0" xmlns:sga="http://sga.mith.org/ns/1.0"
  xml:id="ox-ms_abinger_c56-0049" ulx="0" uly="0" lrx="5364" lry="7104" partOf="#ox-ms_abinger_c56">
