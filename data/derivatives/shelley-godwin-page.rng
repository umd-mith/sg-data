      <ref name="sga-patt.global.attribute.att-value"/>
      <ref name="sga-patt.global.attribute.rend"/>
      <ref name="sga-patt.global.attribute.style"/>
      <ref name="sga-patt.global.linking.attribute.next"/>
      <ref name="sga-patt.global.linking.attribute.prev"/>
      <ref name="sga-patt.global.facs.attribute.facs"/>
      <ref name="sga-patt.coordinated.attribute.start"/>
      <ref name="sga-patt.typed.attributes"/>
