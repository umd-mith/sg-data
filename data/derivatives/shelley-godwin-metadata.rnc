    sga-md_macro.phraseSeq, sga-md_att.global.attributes, empty
  }
sga-md_postCode =
  
  ## (postal code) contains a numerical or alphanumeric code used as part of a postal address to simplify
  ##         sorting or delivery of mail.  [3.5.2. ]
  element postCode { text, sga-md_att.global.attributes, empty }
sga-md_postBox =
  
  ## (postal box or post office box) contains a number or other identifier for some postal delivery point other than a street
  ##     address.  [3.5.2. ]
  element postBox { text, sga-md_att.global.attributes, empty }
sga-md_num =
  
  ## (number) contains a number, written in any form. [3.5.3. ]
  element num {
    sga-md_macro.phraseSeq,
    sga-md_att.global.attributes,
    sga-md_att.ranging.attributes,
    
    ## indicates the type of numeric value.
    ## Suggested values include: 1] cardinal; 2] ordinal; 3] fraction; 4] percentage
    attribute type {
      
      ## absolute number, e.g. 21, 21.5
      "cardinal"
      | 
        ## ordinal number, e.g. 21st
        "ordinal"
      | 
        ## fraction, e.g. one half or three-quarters
        "fraction"
      | 
        ## a percentage
        "percentage"
      | xsd:Name
    }?,
    
    ## supplies the value of the number in standard form.
    attribute value { sga-md_data.numeric }?,
    empty
  }
sga-md_measure =
  
  ## contains a word or phrase referring to some quantity of an object or commodity, usually
  ##         comprising a number, a unit, and a commodity name. [3.5.3. ]
  element measure {
    sga-md_macro.phraseSeq,
    sga-md_att.global.attributes,
    sga-md_att.measurement.attributes,
    
