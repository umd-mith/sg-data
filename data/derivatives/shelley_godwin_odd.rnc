    sgaatt.global.attributes,
    sgaatt.pointing.attributes,
    sgaatt.transcriptional.attributes,
    empty
  }
sgaline =
  
  ## contains the transcription of a topographic line in the source document [11.2.2. ]
  element line {
    (text
     | sgamodel.global
     | sgamodel.segLike
     | sgamodel.hiLike
     | sgazone
     | sgaline
     | sgaadd
     | sgadel
     | sgaadd
     | sgaunclear
     | sgadamage
     | sgahandShift
     | sgamod
     | sgaretrace)*,
    sgaatt.typed.attributes,
    sgaatt.global.attribute.el-target,
    sgaatt.global.attribute.att-target,
    sgaatt.global.attribute.att-value,
    sgaatt.global.attribute.xmlid,
    sgaatt.global.attribute.n,
    sgaatt.global.attribute.style,
    sgaatt.global.attribute.xmlbase,
    sgaatt.global.linking.attribute.corresp,
    sgaatt.global.linking.attribute.next,
    sgaatt.global.linking.attribute.prev,
    sgaatt.global.facs.attribute.facs,
    sgaatt.coordinated.attributes,
    
    ##
    attribute rend {
      
      ##
      "right"
      | 
        ##
        "center"
      | 
        ##
        "left"
      | 
        ##
        "indent1"
      | 
        ##
        "indent2"
      | 
        ##
        "indent3"
      | 
        ##
        "indent4"
      | 
        ##
        "indent5"
    }?,
    empty
  }
sgalistTranspose =
  
  ## supplies a list of transpositions, each of which is indicated at some point in a document typically by means of metamarks. [11.3.4.5. ]
  element listTranspose {
    sgatranspose+, sgaatt.global.attributes, empty
  }
sgametamark =
  
  ## contains or describes any kind of graphic or written signal within a document the function of which is to determine how it should be read rather than forming part of the actual content of the document. [11.3.4.2. ]
  element metamark {
    sgamacro.specialPara,
    sgaatt.spanning.attributes,
    sgaatt.placement.attributes,
    sgaatt.global.attributes,
    
    ##
    attribute function {
      
      ##
      "count"
      | 
        ##
        "insert"
      | 
        ##
        "separate"
      | 
        ##
        "paragraph"
      | 
        ##
        "displacement"
      | 
        ##
        "trasnpose"
    }?,
    
    ## identifies one or more elements to which the function indicated by the metamark applies.
    attribute target {
      list { sgadata.pointer+ }
    }?,
    empty
  }
sgamod =
  
  ## represents any kind of modification identified within a single document. [11.3.4.1. ]
  element mod {
    ((sgamodel.gLike
      | sgamodel.phrase
      | sgamodel.inter
      | sgamodel.global)*)
    >> sch:pattern [
         id = "mod-constraint-modContents"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:mod"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "count(node()) gt 1"
             "The mod element is\x{a}" ~
             "                                        intended to group a series of related changes to the\x{a}" ~
             "                                        manuscript. Thus, mod must have more than one child\x{a}" ~
             "                                        element. If only a single addition or deletion is being\x{a}" ~
             "                                        encoded, mod is not required."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    sgaatt.global.attributes,
    sgaatt.transcriptional.attributes,
    sgaatt.typed.attribute.subtype,
    sgaatt.spanning.attributes,
    
    ##
    attribute type {
      
      ##
      "additions"
      | 
        ##
        "deletions"
    }?,
    empty
  }
sgaretrace =
  
  ## contains a sequence of writing which has been retraced, for example by over-inking, to clarify or fix it. [11.3.4.3. ]
  element retrace {
    sgamacro.paraContent,
    sgaatt.global.attributes,
    sgaatt.spanning.attributes,
    sgaatt.transcriptional.attributes,
    empty
  }
sgatranspose =
  
  ## describes a single textual transposition as an ordered list of at least two pointers specifying the order in which the elements indicated should be re-combined. [11.3.4.5. ]
  element transpose {
    (sgaptr, sgaptr+), sgaatt.global.attributes, empty
