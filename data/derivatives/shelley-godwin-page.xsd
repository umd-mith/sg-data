  <xs:attributeGroup name="sga-patt.pointing.group.attribute.targFunc">
    <xs:attribute name="targFunc">
      <xs:annotation>
        <xs:documentation>(target function) describes the function of each of the values of the
      target attribute of the enclosed link,
      join, or alt tags.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction>
          <xs:simpleType>
            <xs:list>
