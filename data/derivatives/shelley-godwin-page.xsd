    <xs:attribute name="group" type="ns1:sga-pdata.count">
      <xs:annotation>
        <xs:documentation>assigns an arbitrary number to each stretch of damage regarded as forming part of the
        same physical phenomenon.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
