         <ref name="sgaatt.global.attributes"/>
         <ref name="sgaatt.personal.attributes"/>
         <ref name="sgaatt.datable.attributes"/>
         <ref name="sgaatt.editLike.attributes"/>
         <ref name="sgaatt.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="sgars">
      <element name="rs">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(referencing string) contains a general purpose name or referring string. [13.2.1.  3.5.1. ]</a:documentation>
